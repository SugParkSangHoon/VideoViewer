<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:영상뷰어.Views"
                    xmlns:dialogViews="clr-namespace:영상뷰어.Views.Windows"
                    xmlns:dataContext ="clr-namespace:영상뷰어.ViewModels">

    <DataTemplate DataType="{x:Type dataContext:MainViewModel}">
        <views:MainUI/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type dataContext:SateliteAPIResultViewModel}">
        <views:SateliteAPIResultView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type dataContext:SateliteAPISettingsViewModel}">
        <views:SerchBar/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type dataContext:LoginViewModel}">
        <views:LoginView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type dataContext:SignUpViewModel}">
        <views:SignUpView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type dataContext:ImageLoadViewModel}">
        <dialogViews:LoadImageView/>
    </DataTemplate>

</ResourceDictionary>